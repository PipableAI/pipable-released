       "[('SELECT m.first_name, m.last_name\\nFROM alyf_asvs AS a\\nJOIN alyf_members AS m ON a.member_id = m.member_id\\nORDER BY a.weight_daily_avg DESC\\nLIMIT 1;',\n",
       "  'normal'),\n",
       " ('SELECT m.member_id \\nFROM alyf_asvs AS a \\nJOIN alyf_members AS m ON a.member_id = m.member_id \\nWHERE a.daily_hfi > 35;',\n",
       "  'normal'),\n",
       " ('SELECT a.member_id\\nFROM alyf_members a\\nJOIN alyf_asvs b ON a.member_id = b.member_id\\nWHERE b.daily_hfi > 35;',\n",
       "  'normal'),\n",
       " ('SELECT DISTINCT a.member_id\\nFROM alyf_members a\\nJOIN alyf_asvs b ON a.member_id = b.member_id\\nWHERE b.daily_hfi > 35;',\n",
       "  'normal')]"
